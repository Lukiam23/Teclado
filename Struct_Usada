#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <windows.h>
#include <math.h>
    struct Nota
    {
        char nome[5];
        int frequencia;
    };
    struct nome
    {
        char nome[5];
    };
int main()
{

    int i;
    struct Nota e[12];
    struct nome nomes[12] = {"Do","Do#","Re","Re#","Mi","Fa","Fa#","Sol","Sol#","La","La#","Si"};

    for(i=0; i<12; i++)
    {
        strcpy(e[i].nome,nomes[i].nome);
        int c = pow(2,i);
        e[i].frequencia=264*pow(c,1.0/12.0);
    }
    for(i=0; i<12; i++)
    {
        printf("A nota ");
        printf(e[i].nome);
        printf(" tem uma frequencia de %d\n",e[i].frequencia);
        Beep(e[i].frequencia,600);

    }
    }
